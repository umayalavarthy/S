<!DOCTYPE html>
<html>
  <head>
    <title>SaiMedha</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag 'jquery', 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#">Sai Medha</a>
          <div class="nav-collapse">
            <% if user_signed_in? %>
              <ul class="nav">
                <li><%= link_to "home", root_path %>
                <li><%= link_to 'Courses', courses_path %></li>
                <li><%= link_to 'Branches', branches_path %></li>
                <li><%= link_to 'Subjects', subjects_path %></li>
                <li><%= link_to 'Exams', exams_path %>
                <li><%= link_to 'Questions', questions_path %></li>
                <li><%= link_to "Logout",destroy_user_session_path, :method => :delete %></li>
              </ul>
            <% end %>
          </div>
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="span9" style="width :100%"><%= yield %></div>
        <div class="span3">
          <h2></h2>

        </div>
      </div>
    </div>


<%#= yield %>

  </body>
</html>
